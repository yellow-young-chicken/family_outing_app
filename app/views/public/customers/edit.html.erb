<h1>プロフィール編集</h1>

<%= form_with model:@customer do |f| %>
  <label for="inputName">アカウントネーム</label>
  <%= f.text_field :account_name, autofocus: true, id:"inputName", placeholder:"アカウントネーム"%>
  <label for="inputImage">ProfileImage</label>
  <%= f.file_field :profile_image, placeholder: "プロフィール画像", accept: "image/*" %>  
  <%= f.submit "変更を保存" %>
<% end %>
